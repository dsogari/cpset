{
  "$schema": "node_modules/@cspell/cspell-types/cspell.schema.json",
  "version": "0.2",
  "language": "en",
  "dictionaries": ["typescript"],
  "words": ["interactor"],
  "ignoreWords": ["dsogari", "testlib", "publint", "cpset"],
  "ignorePaths": [
    "*.lockb",
    "*.json",
    ".git",
    ".prettierignore",
    "CODEOWNERS",
    "*.{js,ts,jsx,tsx}"
  ],
  "useGitignore": true,
  "enableGlobDot": true
}
